<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="core.Serie" table="Serie" lazy="false">
		<id name="ID" column="ID" type="integer" unsaved-value="0">
			<generator class="native">
			</generator>
		</id>
		<property name="description" column="Description" type="string" length="255" not-null="false" lazy="false"/>
		<property name="repetitions" column="Repetitions" type="string" length="255" not-null="false" lazy="false"/>
		<property name="rest" column="Rest" type="string" length="255" not-null="false" lazy="false"/>
	</class>
</hibernate-mapping>
