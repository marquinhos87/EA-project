<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="notifications.PersonalTrainer" table="PersonalTrainer" lazy="false">
		<id name="username" column="Username" type="string">
			<generator class="assigned">
			</generator>
		</id>
		<set name="ORM_Notifications" lazy="true" cascade="save-update,lock" inverse="false">
			<key column="PersonalTrainerUsername" not-null="false"/>
			<one-to-many class="notifications.Notification"/>
		</set>
	</class>
</hibernate-mapping>
